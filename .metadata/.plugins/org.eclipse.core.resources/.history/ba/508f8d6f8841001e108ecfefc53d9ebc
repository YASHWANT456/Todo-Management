package com.rishu.practise1.controller;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.http.ResponseEntity;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

import com.rishu.practise1.service.ServiceOne;

@RestController
@RequestMapping("/controller1")
public class ControllerOne {
	
	@Autowired
	ServiceOne serviceOne;
	
	
	@GetMapping("/response1")
	public ResponseEntity<String>getResponseOne()
	{
		return serviceOne.getResponseOne();
		
	}
	@GetMapping("/response2")
	public ResponseEntity<List<String>>getResponseTwo()
	{
		return serviceOne.getResponseTwo();
	}
	
	
	

}
